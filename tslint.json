{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "semicolon": [true, "never"],
      "jsdoc-format": true,
      "indent": [true, "spaces", 2],
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "no-console": [false, "log", "error"]
    },
    "rulesDirectory": []
}
